{
    "name": "semihyilmaz/laravel-breeze-bootstrap",
    "description": "Minimal Laravel authentication scaffolding with Blade and Bootstrap.",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/khadegd/larave-breeze-bootstrap/issues",
        "source": "https://github.com/khadegd/laravel-breeze-bootstrap"
    },
    "authors": [
        {
            "name": "Semih YILMAZ",
            "email": "semy@semihyilmaz.com"
        },
        {
            "name": "Ganesh Khade",
            "email": "ganesh.khade@outlook.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/filesystem": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/validation": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
